{
  "terminal.integrated.profiles.linux": {
    "ros2": {
      "path": "bash",
      "args": [
        "-c",
        "source install/setup.bash && bash"
      ],
      "icon": "gear",
      "color": "terminal.ansiGreen",
      "overrideName": true
    }
  },
  "terminal.integrated.defaultProfile.linux": "ros2",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/flutter/ephemeral": true,
    "**/Flutter/ephemeral": true,
    "**/.symlinks": true,
    "**/.plugin_symlinks": true,
    "**/*.meta": true
  },
  "hide-files.files": [],
}